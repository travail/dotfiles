[user]
	name  = travail
	email = sawa.tomoyuki@gmail.com
[alias]
    ci  = commit -a
    co  = checkout
    st  = status
    url = config --get remote.origin.url
    d1  = diff HEAD^
    d2  = diff HEAD^^
    d3  = diff HEAD^^^
    d4  = diff HEAD^^^^
    d5  = diff HEAD^^^^^
    d10 = diff HEAD^^^^^^^^^^
[color]
	ui     = auto
    diff   = auto
    status = auto
    banch  = auto
[remote "origin"]
[include]
	path = ~/.gitconfig.local
